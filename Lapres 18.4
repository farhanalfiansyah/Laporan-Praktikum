#include <stdio.h>
int maxmin(int,int,int);
int main()
{   int n,max,min;
    printf("Masukkan N : ");scanf("%d",&n);
    maxmin(max,min,n);
}
int maxmin(int x,int y,int n)
{   int i,array[100];
    x=0; y=0;
    for (i=0;i<n;i++)
    {
        printf("Masukkan data ke %d : ",i+1);scanf("%d",&array[i]);
        if (array[i]>x || x==0)
        x=array[i];
        if (array[i]<y || y==0)
        y=array[i];
    }
    printf("Nilai maximum dan minimum: %d & %d",x,y);
}
